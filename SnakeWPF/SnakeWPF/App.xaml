<Application x:Class="SnakeWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SnakeWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#8ED081</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#DCE2AA</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#588B8B</SolidColorBrush>
        <SolidColorBrush x:Key ="TextColor">#4B543B</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#B4D2BA </SolidColorBrush>
        <FontFamily x:Key="MainFont:">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
