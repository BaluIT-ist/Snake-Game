        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeWPF"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="500" Width="450">
    <Grid Background="#96C5B0">
        <Button x:Name="Resolution_Btn"
                Height="45"
                Width="200"
                Content="Window Mode/ FullScreen"
                Foreground="Black" Margin="100,113,100,276"
                Background="#56EEF4">
        </Button>
        <Button x:Name="ChangeFruitColor_btn"
                Height="45"
                Width="200"
                Content="Change Fruit Color"
                Foreground="Black" Margin="100,80,100,109"
                 Background="#56EEF4">


            
        </Button>
    </Grid>
</Window>
